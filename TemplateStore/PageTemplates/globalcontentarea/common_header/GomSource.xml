<?xml version="1.0" encoding="UTF-8"?>
<CMS_MODULE>

  <FS_LIST name="st_links" hFill="yes">
    <DATASOURCE type="inline" useLanguages="yes">
      <LABELS>
        <LABEL lang="*">#item.text</LABEL>
      </LABELS>
      <ACTIONS>
        <ACTION name="ADD"/>
        <ACTION name="REMOVE"/>
        <ACTION name="UP"/>
        <ACTION name="DOWN"/>
        <ACTION name="EDIT"/>
      </ACTIONS>
      <COLUMNS>
        <COLUMN show="no">#identifier</COLUMN>
      </COLUMNS>
      <LAYOUT>
        <ADD component="toolbar" constraint="top"/>
        <ADD component="overview" constraint="center"/>
        <ADD component="stackedview" constraint="hide"/>
      </LAYOUT>
      <TEMPLATES source="linktemplates">
        <TEMPLATE uid="globallinksinternal"/>
      </TEMPLATES>
    </DATASOURCE>
    <LANGINFOS>
      <LANGINFO lang="*" label="Global links" description="Insert your global links."/>
      <LANGINFO lang="DE" label="Globale Verweise" description="Geben Sie Ihre globalen Verweise ein."/>
    </LANGINFOS>
  </FS_LIST>

</CMS_MODULE>